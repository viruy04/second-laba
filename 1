#include <stdio.h>

int main() {
    
    float x, y;
    printf("Vvedite znachenie dlya x ");
    scanf("%f", &x);
    printf("Vvedite znachenie dlya y ");
    scanf("%f", &y);
    
    if ((y > 0) && (y < 0.2) && (x * x + y * y < 1)) {
        printf("Tochka prinadlezit oblasti");
    } else if ((y < 0) && (y > 0.2) || (x * x + y * y > 1)) {
        printf("Tochka ne prinadlezit oblasti");
    } else if ((y == 0) || (x * x + y * y < 1)) {
        printf("Tochka lezit na granice okruznosti");
    } else if ((y == 0.2) && (x * x + y * y < 1)) {
        printf("Tochka lezit na granice okruznosti");
    } else if ((y > 0) && (y < 0.2) && (x * x + y * y == 1)) {
        printf("Tochka lezit na granice okruznosti");
    }

    return 0;
}
